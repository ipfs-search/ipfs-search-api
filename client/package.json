{
  "name": "ipfs-search-client",
  "version": "1.0.2",
  "description": "Official TypeScript/JavaScript API client for ipfs-search.com, generated from the OpenAPI 3.0 definition.",
  "author": "Mathijs de Bruin",
  "keywords": [
    "fetch",
    "typescript",
    "swagger-client",
    "ipfs-search-client",
    "ipfs",
    "search"
  ],
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ipfs-search/ipfs-search-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs-search/ipfs-search-api.git"
  },
  "homepage": "https://github.com/ipfs-search/ipfs-search-api/tree/master/client#readme",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist/",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/node": "^16.11.21",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "cross-fetch": "^3.1.4"
  },
  "engines": {
    "node": "16.x"
  }
}
